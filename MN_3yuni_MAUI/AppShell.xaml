<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MN_3yuni_MAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MN_3yuni_MAUI.MVVM.Views"
    Background="{StaticResource BackgroundGray}"
    Shell.TabBarIsVisible="False">
    <TabBar>
        <ShellContent ContentTemplate="{DataTemplate local:Home}" Route="Home" />
        <ShellContent ContentTemplate="{DataTemplate local:OrderPage}" Route="OrderPage" />
        <ShellContent ContentTemplate="{DataTemplate local:ChatPage}" Route="ChatPage" />
        <ShellContent ContentTemplate="{DataTemplate local:ProfilePage}" Route="ProfilePage" />
        <ShellContent ContentTemplate="{DataTemplate local:WalletPage}" Route="WalletPage" />
    </TabBar>


</Shell>
